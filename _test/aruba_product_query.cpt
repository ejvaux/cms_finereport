<TableData name="get_data_aruba" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="P_INVENTORY_ITEM_ID"/>
<O>
<![CDATA[0ANK1600KP]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[ORACLE TEST]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select s.*,


(select max(case
                         when COO1 is not null then
                          COO1 || case
                            when COO2 is not null then
                             '/' || COO2 || case
                               when COO3 is not null then
                                '/' || COO3 || case
                                  when COO4 is not null then
                                   '/' || COO4 || case
                                     when COO5 is not null then
                                      '/' || COO5
                                     else
                                      null
                                   end
                                  else
                                   null
                                end
                               else
                                null
                             end
                            else
                             null
                          end
                         else
                          null
                       end) COO
              from YSC.siv_items_resume
             where sercomm_item = s.sub_item_code
               and site =
                   (select organization_code
                      from mtl_parameters
                     where organization_id = 83)) COO,

--manufacturer          
           (select MAKER
              from (SELECT ROW_NUMBER() OVER(PARTITION BY B.SEGMENT1 ORDER BY A.MANUFACTURER_NAME) AS SEQ,
                           B.SEGMENT1,
                           A.MANUFACTURER_NAME || '-' || C.DESCRIPTION as MAKER
                      FROM MTL_MFG_PART_NUMBERS_ALL_V A,
                           MTL_SYSTEM_ITEMS_B         B,
                           MTL_MANUFACTURERS          C
                     WHERE (A.INVENTORY_ITEM_ID = B.INVENTORY_ITEM_ID AND
                           B.ORGANIZATION_ID = 83)
                       AND A.MANUFACTURER_NAME = C.MANUFACTURER_NAME) aa
             where aa.SEQ = '1'
               and aa.segment1 = s.sub_item_code) MANUFACTURER,
               
--MPN COLUMN
           (select MPN
              from (SELECT ROW_NUMBER() OVER(PARTITION BY B.SEGMENT1 ORDER BY A.MANUFACTURER_NAME) AS SEQ,
                           B.SEGMENT1,
                           A.MFG_PART_NUM as MPN
                      FROM MTL_MFG_PART_NUMBERS_ALL_V A,
                           MTL_SYSTEM_ITEMS_B         B,
                           MTL_MANUFACTURERS          C
                     WHERE (A.INVENTORY_ITEM_ID = B.INVENTORY_ITEM_ID AND
                           B.ORGANIZATION_ID = 83)
                       AND A.MANUFACTURER_NAME = C.MANUFACTURER_NAME) aa
             where aa.SEQ = '1'
               and aa.segment1 = s.sub_item_code) MPN
     
                      
from 
(SELECT msi.segment1 as Parent_material_coding,
msi.inventory_item_id,
msi.description as Parent_item_description,
msi1.segment1 as Sub_item_code,
msi1.inventory_item_id as sub_item_id,
msi1.description as Sub_item_description,
bic.disable_date
FROM mtl_system_items_b msi,
bom_bill_of_materials bom,
bom_inventory_components bic,
mtl_system_items_b msi1
WHERE 1 = 1
AND msi.organization_id = 83
AND msi.inventory_item_id = bom.assembly_item_id
AND msi.organization_id = bom.organization_id
AND bom.organization_id = 83
AND bom.alternate_bom_designator IS NULL
AND bom.bill_sequence_id = bic.bill_sequence_id
AND bic.disable_date IS NULL
AND bic.component_item_id = msi1.inventory_item_id
AND msi1.organization_id = 83
AND msi.segment1 in ('${P_INVENTORY_ITEM_ID}')) s
order by s.parent_material_coding]]></Query>
<PageQuery>
<![CDATA[]]></PageQuery>
