<TableData name="get_trade_term" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="P_INVENTORY_ITEM_ID"/>
<O>
<![CDATA[0ANK1600KP]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[ORACLE TEST]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select s.*,
               --trade_term
               (select 
                   x.fob_lookup_code
              from (select msi.segment1,
                           assa.fob_lookup_code,
                           row_number() over(partition by msi.segment1, asl.segment1, assa.vendor_site_code order by pha.approved_date desc, pha.creation_date desc) as seq_no
                      FROM mtl_system_items      msi,
                           po_headers_all        pha,
                           po_lines_all          pla,
                           po_line_locations_all plla,
                           ap_suppliers          asl,
                           ap_supplier_sites_all assa,
                           ap_terms              at,
                           apps.po_buyers_v      pb,
                           apps.po_buyers_v      pb2
                     where pha.po_header_id = pla.po_header_id
                       and pha.po_header_id = plla.po_header_id
                       and pla.po_line_id = plla.po_line_id
                       and pha.type_lookup_code = 'BLANKET'
                       and plla.ship_to_organization_id = msi.organization_id
                       and pla.item_id = msi.inventory_item_id
                       and pha.vendor_id = asl.vendor_id
                       and pha.vendor_site_id = assa.vendor_site_id
                       and at.term_id = assa.terms_id(+)
                       and pha.agent_id = pb.employee_id(+)
                       and msi.buyer_id = pb2.employee_id(+)
                       AND PLLA.SHIPMENT_TYPE = 'PRICE BREAK'
                       AND plla.SHIPMENT_NUM = 1
                       and (msi.PLANNING_MAKE_BUY_CODE = 2 or
                           msi.segment1 like '584%')
                       and (pha.start_date =
                           (select distinct max(pha1.start_date)
                               from po_headers_all        pha1,
                                    po_lines_all          pla1,
                                    po_line_locations_all plla1
                              where pha1.po_header_id = pla1.po_header_id
                                and pha1.po_header_id = plla1.po_header_id
                                and pla1.po_line_id = plla1.po_line_id
                                AND PLLA1.SHIPMENT_TYPE = 'PRICE BREAK'
                                AND plla.SHIPMENT_NUM = 1
                                and plla1.ship_to_organization_id =
                                    plla.ship_to_organization_id
                                and pla1.item_id = pla.item_id
                                and pha1.vendor_id = pha.vendor_id
                                and pha1.vendor_site_id = pha.vendor_site_id
                                and (PLA1.CANCEL_FLAG = 'N' or
                                    PLA1.CANCEL_FLAG is null)
                                and (PLA1.closed_code = 'OPEN' or
                                    PLA1.closed_code is null)
                                and nvl(PHA1.END_DATE, sysdate) >= sysdate
                                and (PLA.CANCEL_FLAG = 'N' or
                                    PLA.CANCEL_FLAG is null)
                                and (PLA.closed_code = 'OPEN' or
                                    PLA.closed_code is null)
                                and nvl(PHA.END_DATE, sysdate) >= sysdate
                                   
                                and trunc(nvl(pla.closed_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(plla.end_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(pla1.closed_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(plla1.end_date, (sysdate + 1))) >=
                                    trunc(sysdate)))
                          --AND MSI.SEGMENT1 = SB.SEGMENT1 
                       and plla.ship_to_organization_id = 83
                     order by pha.start_date desc) x
             where x.seq_no = 1
               and x.segment1 = s.sub_item_code
               and rownum = 1) TRADE_TERM,

                 --trade_term_flag -- Y or N
              (select 
                   (case
                     when x.fob_lookup_code in
                          ('EXW',
                           'FCA',
                           'FCA SINGAPORE',
                           'FCA-HK',
                           'FCA-SH',
                           'FCA-TW',
                           'FCA-蘇州物流園',
                           'FOB-HK',
                           'FOB-JP',
                           'FOB-KOREA',
                           'FOB-SG',
                           'FOB-SH',
                           'FOB-SP',
                           'FOB-TW',
                           'FOB-其他') then
                      'Y'
                     else
                      'N'
                   end) 
              from (select msi.segment1,
                           assa.fob_lookup_code,
                           row_number() over(partition by msi.segment1, asl.segment1, assa.vendor_site_code order by pha.approved_date desc, pha.creation_date desc) as seq_no
                      FROM mtl_system_items      msi,
                           po_headers_all        pha,
                           po_lines_all          pla,
                           po_line_locations_all plla,
                           ap_suppliers          asl,
                           ap_supplier_sites_all assa,
                           ap_terms              at,
                           apps.po_buyers_v      pb,
                           apps.po_buyers_v      pb2
                     where pha.po_header_id = pla.po_header_id
                       and pha.po_header_id = plla.po_header_id
                       and pla.po_line_id = plla.po_line_id
                       and pha.type_lookup_code = 'BLANKET'
                       and plla.ship_to_organization_id = msi.organization_id
                       and pla.item_id = msi.inventory_item_id
                       and pha.vendor_id = asl.vendor_id
                       and pha.vendor_site_id = assa.vendor_site_id
                       and at.term_id = assa.terms_id(+)
                       and pha.agent_id = pb.employee_id(+)
                       and msi.buyer_id = pb2.employee_id(+)
                       AND PLLA.SHIPMENT_TYPE = 'PRICE BREAK'
                       AND plla.SHIPMENT_NUM = 1
                       and (msi.PLANNING_MAKE_BUY_CODE = 2 or
                           msi.segment1 like '584%')
                       and (pha.start_date =
                           (select distinct max(pha1.start_date)
                               from po_headers_all        pha1,
                                    po_lines_all          pla1,
                                    po_line_locations_all plla1
                              where pha1.po_header_id = pla1.po_header_id
                                and pha1.po_header_id = plla1.po_header_id
                                and pla1.po_line_id = plla1.po_line_id
                                AND PLLA1.SHIPMENT_TYPE = 'PRICE BREAK'
                                AND plla.SHIPMENT_NUM = 1
                                and plla1.ship_to_organization_id =
                                    plla.ship_to_organization_id
                                and pla1.item_id = pla.item_id
                                and pha1.vendor_id = pha.vendor_id
                                and pha1.vendor_site_id = pha.vendor_site_id
                                and (PLA1.CANCEL_FLAG = 'N' or
                                    PLA1.CANCEL_FLAG is null)
                                and (PLA1.closed_code = 'OPEN' or
                                    PLA1.closed_code is null)
                                and nvl(PHA1.END_DATE, sysdate) >= sysdate
                                and (PLA.CANCEL_FLAG = 'N' or
                                    PLA.CANCEL_FLAG is null)
                                and (PLA.closed_code = 'OPEN' or
                                    PLA.closed_code is null)
                                and nvl(PHA.END_DATE, sysdate) >= sysdate
                                   
                                and trunc(nvl(pla.closed_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(plla.end_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(pla1.closed_date, (sysdate + 1))) >=
                                    trunc(sysdate)
                                   
                                and trunc(nvl(plla1.end_date, (sysdate + 1))) >=
                                    trunc(sysdate)))
                          --AND MSI.SEGMENT1 = SB.SEGMENT1 
                       and plla.ship_to_organization_id = 83
                     order by pha.start_date desc) x
             where x.seq_no = 1
               and x.segment1 = s.sub_item_code
               and rownum = 1) TRADE_TERM_FLAG 

                  
                   
                      
from 
(SELECT msi.segment1 as Parent_material_coding,
msi.inventory_item_id,
msi.description as Parent_item_description,
msi1.segment1 as Sub_item_code,
msi1.inventory_item_id as sub_item_id,
msi1.description as Sub_item_description,
bic.disable_date
FROM mtl_system_items_b msi,
bom_bill_of_materials bom,
bom_inventory_components bic,
mtl_system_items_b msi1
WHERE 1 = 1
AND msi.organization_id = 83
AND msi.inventory_item_id = bom.assembly_item_id
AND msi.organization_id = bom.organization_id
AND bom.organization_id = 83
AND bom.alternate_bom_designator IS NULL
AND bom.bill_sequence_id = bic.bill_sequence_id
AND bic.disable_date IS NULL
AND bic.component_item_id = msi1.inventory_item_id
AND msi1.organization_id = 83
AND msi.segment1 in ('${P_INVENTORY_ITEM_ID}')) s
order by s.parent_material_coding]]></Query>
